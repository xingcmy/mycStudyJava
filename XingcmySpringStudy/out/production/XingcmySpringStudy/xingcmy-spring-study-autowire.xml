<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="autowiringServerName" class="main.java.com.study.AutowireBean.AutowiringServerName" autowire="byName"></bean>

    <bean id="autowiringServerType" class="main.java.com.study.AutowireBean.AutowiringServerType" autowire="byType"></bean>

    <bean id="autowiringServerConstructor" class="main.java.com.study.AutowireBean.AutowiringServerConstructor" autowire="constructor"></bean>

    <bean id="autowiringDemo" class="main.java.com.study.AutowireBean.AutowiringDemo"></bean>
</beans>